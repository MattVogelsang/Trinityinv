{
  "name": "QuoteRequest",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Full name of the requester"
    },
    "email": {
      "type": "string",
      "format": "email",
      "description": "Email address"
    },
    "phone": {
      "type": "string",
      "description": "Phone number"
    },
    "insurance_type": {
      "type": "string",
      "enum": [
        "commercial",
        "marine",
        "hotel_motel",
        "condo",
        "homeowners",
        "other"
      ],
      "description": "Type of insurance interested in"
    },
    "message": {
      "type": "string",
      "description": "Additional details or questions"
    },
    "status": {
      "type": "string",
      "enum": [
        "new",
        "contacted",
        "quoted",
        "closed"
      ],
      "default": "new"
    }
  },
  "required": [
    "name",
    "email"
  ]
}

